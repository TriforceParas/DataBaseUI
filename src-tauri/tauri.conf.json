{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "SQL UI",
  "version": "0.1.0",
  "identifier": "com.sqlui.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "SQL UI",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "minWidth": 600,
        "minHeight": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "rpm",
      "msi",
      "dmg"
    ],
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFERjg4QTMyNzQwNEUxNjQKUldSazRRUjBNb3I0SFVpREFYUlp5cXEvVTJOaGVja2FCRkRETnlOK0xkUlZTeGIxcjZMclgxaXIK",
      "endpoints": [
        "https://github.com/TriforceParas/DataBaseUI/releases/latest/download/latest.json"
      ]
    }
  }
}